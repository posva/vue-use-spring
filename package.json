{
  "name": "vue-use-spring",
  "version": "0.1.1",
  "description": "Natural animations with springs",
  "main": "dist/vue-use-spring.cjs.js",
  "unpkg": "dist/vue-use-spring.global.js",
  "jsdelivr": "dist/vue-use-spring.global.js",
  "module": "dist/vue-use-spring.esm-bundler.js",
  "types": "dist/vue-use-spring.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "posva13@gmail.com"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose",
    "release": "bash scripts/release.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "size": "size-limit",
    "lint": "prettier -c --parser typescript \"{src,__tests__,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "yarn run lint --write",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "jest --coverage",
    "test": "yarn run test:types && yarn run test:unit && yarn run build && yarn run build:dts"
  },
  "files": [
    "dist/*.js",
    "dist/vue-use-spring.d.ts",
    "LICENSE",
    "README.md"
  ],
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "7.25.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.1.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.5.2",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/test-utils": "^2.0.0",
    "@vueuse/core": "^8.6.0",
    "codecov": "^3.8.3",
    "conventional-changelog-cli": "^2.2.2",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "lodash-es": "^4.17.21",
    "pascalcase": "^1.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.5",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "size-limit": [
    {
      "path": "size-checks/basic.js"
    }
  ],
  "peerDependencies": {
    "@vue/composition-api": "^1.4.0",
    "vue": "^2.6.12 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-use-spring.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-use-spring/issues"
  },
  "homepage": "https://github.com/posva/vue-use-spring#readme",
  "dependencies": {
    "vue-demi": "^0.13.1"
  }
}
